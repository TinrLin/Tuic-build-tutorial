{
    "relay": {
        "server": "www.example.com",  //example.com替换为你的域名
        "port": 443,
        "token": "Password",  //你的密码
        "udp_relay_mode": "quic",
        "congestion_controller": "bbr",
        "heartbeat_interval": 10000,
        "alpn": ["h3"],
        "disable_sni": false,
        "reduce_rtt": true,
        "request_timeout": 8000,
        "max_udp_relay_packet_size": 1500
    },
    "local": {
        "port": 1080,
        "ip": "127.0.0.1"
    },
    "log_level": "info"
}
